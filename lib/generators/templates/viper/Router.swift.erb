import Foundation
import UIKit
import ApiHB

class <%= @className %>Router: BaseRouter, <%= @className %>RouterProtocol {


    //----------------------------
    // MARK: - Launcher Initializer
    //----------------------------
    @discardableResult
    static func launchModule(product: Product) -> UIViewController? {
        if let view = StoryboardHandler.instantiateViewController(.contractInfo) as? <%= @className %>ViewController {
                    
            var interactor: <%= @className %>InteractorProtocol = <%= @className %>Interactor()
            let router: <%= @className %>RouterProtocol = <%= @className %>Router()
            var presenter: <%= @className %>PresenterProtocol = <%= @className %>Presenter(product: product)

            view.presenter = presenter
            presenter.view = view
            presenter.router = router
            presenter.interactor = interactor
            interactor.presenter = presenter
                    
            return view
        }
        
        return nil
    }
    
    //----------------------------
    // MARK: - Navigation methods
    //----------------------------
}
